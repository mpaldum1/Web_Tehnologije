    izbor:
        above unnesi-1 2px
        height 19px
        inside unos 1px top, 47px right
        right-of labela-1 0px
        width 51px

    broj-1, slobodan-1, broj-2, zauzet-1, broj-3, slobodan-2:
        width 71px

    unnesi-1, prethodni, sljedeci:
        height 25px

