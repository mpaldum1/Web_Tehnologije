    unnesi-2, unnesi-3:
        height 29px

    a:
        inside elementLIste-1 0px top left bottom
        width 50px

    elementLIste-1, a, elementLIste-2, elementLIste-3, elementLIste-4, elementLIste-5:
        height 18px

    slobodan-1:
        below broj-1 0px
        inside prvi-5 2px left right bottom

    labela-1:
        above labela-2 0px
        inside unos 0px top left

    labela-1, labela-2, labela-3, labela-4:
        height 30px

    slobodan-2:
        inside prvi-7 2px left right bottom

    labela-2:
        above labela-3 0px

    labela-3:
        above labela-4 0px

    elementLIste-3:
        above elementLIste-4 1px
